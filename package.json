{
    "name": "legal-contract-analyzer",
    "version": "1.0.0",
    "description": "AI-powered legal contract analysis platform using MindsDB Knowledge Bases",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
